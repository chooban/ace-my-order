{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "es6"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "baseUrl": "./src/",
    "typeRoots": [
      "typings/"
    ]
  },
  "include": [
    "src/",
    "typings"
  ],
  "exclude": [
    "plugins",
    "gatsby*",
    "node_modules",
    "public",
    ".cache"
  ]
}
